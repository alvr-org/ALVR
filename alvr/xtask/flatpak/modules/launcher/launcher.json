{
  "name": "launcher",
  "buildsystem": "simple",
  "build-commands": [
    "mkdir -p /app/utils/alvr/tmp",
    "wget https://github.com/alvr-org/ALVR/releases/download/v20.9.1/alvr_launcher_linux.tar.gz -O /app/utils/alvr/tmp/alvr_launcher.tgz",
    "tar xvf /app/utils/alvr/tmp/alvr_launcher.tgz -C /app/utils/alvr/tmp/",
    "ls -al /app/utils/alvr/tmp/alvr_launcher_linux"
  ]
}
